<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>ai.openclaw.chief-of-staff.backup-sqlite</string>

    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>-lc</string>
      <string>"$HOME/.openclaw/workspace/ops/backup_sqlite.sh"</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key>
      <integer>3</integer>
      <key>Minute</key>
      <integer>17</integer>
    </dict>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/openclaw-chief-of-staff-backup-sqlite.out.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/openclaw-chief-of-staff-backup-sqlite.err.log</string>
  </dict>
</plist>
